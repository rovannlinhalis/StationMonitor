<body>
    <nav class="navbar navbar-expand-md navbar-dark fixed-top" style="background-color: #ffa69e;">
        <a class="navbar-brand mr-1" asp-area="" asp-controller="Home" asp-action="Index">
           Station Monitor
        </a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarTogglerDemo01" aria-controls="navbarTogglerDemo01" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarTogglerDemo01">
            
                <ul class="navbar-nav mr-auto mt-2 mt-lg-0">
                    <li class="nav-item"><a class="nav-link" asp-area="" asp-controller="Stations" asp-action="Index">My Stations</a></li>
                    <li class="nav-item"><a class="nav-link" asp-area="" asp-controller="Groups" asp-action="Index">My Groups</a></li>
                    @*<li><a asp-area="" asp-controller="Estacoes" asp-action="Contact">Contact</a></li>*@
                </ul>
                <partial name="_LoginPartial" />
            
        </div>

    </nav>

    <partial name="_CookieConsentPartial" />

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; 2018 - StationMonitor</p>
        </footer>
    </div>



    <script src="~/lib/jquery/jquery.js"></script>
    <script src="~/lib/popper/umd/popper.js"></script>
    <script src="~/lib/bootstrap/js/bootstrap.js"></script>
    <script src="~/lib/fontawesome/js/fontawesome-iconpicker.js"></script>
    
    
